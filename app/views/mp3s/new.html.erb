<h1>Mp3#new</h1>

<%= form_for @mp3 do |f| %>
<%= f.label :title %>
<%= f.text_field :title %><br>

<%= f.label :file_location %>
<%= f.text_field :file_location %><br>

<%= f.label :chart_id %>
<%= f.text_field :chart_id %><br>
 
<%= f.label :youtubelink %>
<%= f.text_field :youtubelink %><br>

<%= f.submit %>
<% end %>